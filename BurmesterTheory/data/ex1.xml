<poses author="Gen Nishida" version="1.0" date="6/21/2017">
	<pose>
		<point x="1" y="1"/>
		<point x="2" y="1"/>
	</pose>
	<pose>
		<point x="2" y="0.5"/>
		<point x="3" y="0.51"/>
	</pose>
	<pose>
		<point x="3" y="1.5"/>
		<point x="3.70710678" y="2.20710678"/>
	</pose>
	<pose>
		<point x="2" y="2"/>
		<point x="2" y="3"/>
	</pose>
</poses>
